<div class="page-header">
<h3>Cadastro Cliente</h3>
</div>
<div class="contactUs">
<h4>Cadastro do Cliente</h4>
<form class="formContactUs" role="form"
  action="/cliente/save?form=contactus" method="POST">

  <input type="hidden" name="_csrf" value="{{csrf}}">
  {{#each data}}
  <div class="form-group">
    <label for="fieldEmail" class="control-label col-sm-2">Email</label>
    <input type="email" class="form-control" id="fieldEmail" name="email" value={{this.EMAIL}}>
  </div> <!-- End of form-group -->

  <div class="form-group">
    <label for="fieldQues" class="control-label col-sm-2">Nome</label>
    <input type="text" class="form-control" id="fieldQues"name="name" value={{this.NOME}}>
  </div> <!-- End of form-group -->

  <div class="form-group">
    <label for="fieldEnd" class="control-label col-sm-2">Endereco</label>
    <input type="text" class="form-control" id="fieldEnd" name="endereco" value={{this.ENDERECO}}>
  </div> <!-- End of form-group -->

  <div class="form-group">
      <input type="hidden" class="form-control" id="fieldOid" name="oid" value={{this.OID}}>
  </div> <!-- End of form-group -->
  {{else}}
   <div class="form-group">
    <label for="fieldEmail" class="control-label col-sm-2">Email</label>
    <input type="email" class="form-control" id="fieldEmail" name="email">
  </div> <!-- End of form-group -->

  <div class="form-group">
    <label for="fieldQues" class="control-label col-sm-2">Nome</label>
    <input type="text" class="form-control" id="fieldQues" name="name" >
  </div> <!-- End of form-group -->

  <div class="form-group">
    <label for="fieldEnd" class="control-label col-sm-2">Endereco</label>
    <input type="text" class="form-control" id="fieldEnd" name="endereco">
  </div> <!-- End of form-group -->

  <div class="form-group">
      <input type="hidden" class="form-control" id="fieldOid" name="oid" value="0">
  </div> <!-- End of form-group -->

  {{/each}}
  <div class="form-group">
    <div class="col-sm-10">
      <button type="submit" class="btn btn-default">Submit</button>
    </div>
  </div> <!-- End of form-group -->

</form>

</div> <!-- End of contactUs -->
