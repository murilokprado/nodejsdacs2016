<div class="page-header">
<h3>Cadastro Cliente</h3>
</div>
<div class="contactUs">
<h4>Cadastro do Cliente</h4>
<form class="formContactUs" role="form"
  action="/cliente/save?form=contactus" method="POST">

  <input type="hidden" name="_csrf" value="{{csrf}}">
  {{#each data}}
  <div class="form-group">
    <label for="fieldEmail" class="control-label col-sm-2">
    Email</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="fieldEmail"
      name="email" value={{this.EMAIL}}>
    </div>
  </div> <!-- End of form-group -->

  <div class="form-group">
    <label for="fieldQues" class="control-label col-sm-2">
    Nome</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="fieldQues"
      name="name" value={{this.NAME}}>
    </div>
  </div> <!-- End of form-group -->

  <div class="form-group">
    <label for="fieldEnd" class="control-label col-sm-2">
    Endereco</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="fieldEnd"
      name="endereco" value={{this.ENDERECO}}>
    </div>
  </div> <!-- End of form-group -->

  <div class="form-group">
    <div class="col-sm-10">
      <input type="hidden" class="form-control" id="fieldOid"
      name="oid" value={{this.OID}}>
    </div>
  </div> <!-- End of form-group -->
  {{else}}
  <div class="form-group">
    <label for="fieldEmail" class="control-label col-sm-2">
    Email</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="fieldEmail"
      name="email" >
    </div>
  </div> <!-- End of form-group -->

  <div class="form-group">
    <label for="fieldQues" class="control-label col-sm-2">
    Nome</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="fieldQues"
      name="name" >
    </div>
  </div> <!-- End of form-group -->

  <div class="form-group">
    <label for="fieldEnd" class="control-label col-sm-2">
    Endereco</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="fieldEnd"
      name="endereco" >
    </div>
  </div> <!-- End of form-group -->

  <div class="form-group">
    <div class="col-sm-10">
      <input type="hidden" class="form-control" id="fieldOid"
      name="oid" value="0">
    </div>
  </div> <!-- End of form-group -->
  {{/each}}
  <div class="form-group">
    <div class="col-sm-10">
      <button type="submit" class="btn btn-default">Submit</button>
    </div>
  </div> <!-- End of form-group -->

</form>

</div> <!-- End of contactUs -->
